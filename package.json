{
  "name": "stream-chat",
  "version": "1.0.0",
  "description": "Connects with multiple stream services to and combines the log",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev:client": "webpack --config webpack/webpack.config.dev.js",
    "build:dev:server": "webpack --config webpack/webpack.config.server.js",
    "build:dev": "npm run clean && npm run build:dev:server && npm run build:dev:client",
    "build:prod:client": "NODE_ENV=production DEBUG=-* webpack --config webpack/webpack.config.prod.js -p",
    "build:prod:server": "NODE_ENV=production webpack --config webpack/webpack.config.server.js",
    "sentry:io:deploy": "node scripts/sentry.io.deploy",
    "prebuild": "npm run clean",
    "build": "npm run build:prod:server && npm run build:prod:client",
    "prestart:release": "npm run build",
    "start:release": "npm run sentry:io:deploy && NODE_ENV=production node build/server.js",
    "prestart": "npm run clean",
    "start": "NODE_ENV=development node scripts/dev.js",
    "dev": "NODE_ENV=development node scripts/dev.js",
    "now-build": "npm run build:prod:server && npm run build:prod:client",
    "now-start": "NODE_ENV=production node build/server.js"
  },
